import org.code.neighborhood.*;

public class RowPainter extends Painter {
  public RowPainter(int yPos){
     super(0, yPos, "east", 100);    
  }
  public void paintLine(int[] line) {
    
  }
  public void turnRight() {
    turnLeft();
    turnLeft();
    turnLeft();
  }

  public void moveSpaces(int spaces){
    for(int i=0; i < spaces; i++){
      this.move();
    }
  }
}
